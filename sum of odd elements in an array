//sum of odd numbers in an array
#include<stdio.h>
int main()
{
    int ar[50];
    int i,n,result=0;
    printf("enter the no of elements in the array:");
    scanf("%d",&n);
    printf("enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&ar[i]);
    }
    for(i=0;i<n;i++)
    {
        if(ar[i]%2!=0)
            result=result+ar[i];
    }
printf("sum of odd elements:%d",result);
    return 0;
}
