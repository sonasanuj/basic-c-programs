#include<stdio.h>
int main()
{
    int mark;
    printf("enter the mark out of 100");
    scanf("%d",&mark);
    if(mark>=90)
        printf("s");
    else if(mark>=80 && mark<=89)
        printf("A+");
    else if(mark>=70 && mark<=79)
        printf("A");
    else if(mark>=65 && mark<=69)
        printf("B+");
    else if(mark>=60 && mark<=64)
        printf("B");
    else if(mark>=55 && mark<=59)
        printf("C+");
    else if(mark>=50 && mark<=54)
        printf("C");
    else if(mark>=45 && mark<=49)
        printf("c");
    else
        printf("D");
return 0;
}
