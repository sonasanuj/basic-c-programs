#include<stdio.h>
int main()
{
    int ar[100],n,key,i,found=0;
    printf("enter number of elements:");
    scanf("%d",&n);
    printf("enter array elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&ar[i]);
    }
    printf("enter element to search: ");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(ar[i]==key)
        {
            printf("element found");
            found=1;
            break;
        }
    }
    if(found==0)
    {
        printf("element not found");
    }
    return 0;
}
