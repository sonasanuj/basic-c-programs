#include <stdio.h>
int main()
{
  int mat1[10][10],t[10][10];
  int i,j,r,c;
  printf("enter the order of the matrix:\n");
  scanf("%d%d",&r,&c);
  printf("enter the elements:\n");
  for(i=0;i<r;i++)
  {
    for(j=0;j<c;j++)
    {
      scanf("%d",&mat1[i][j]);
    }
  }
  for(i=0;i<r;i++)
  {
    for(j=0;j<c;j++)
    t[j][i]=mat1[i][j];
  }
  printf("transpose of the matrix\n");
  for(i=0;i<c;i++)
  {
    for(j=0;j<r;j++)
    {
    printf("%d",t[i][j]);
    }
      printf("\n");
  }
  
  return 0;
  
  
}
